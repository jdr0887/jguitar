<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.*?>
<?import javafx.scene.image.*?>

<Pane prefWidth="400" prefHeight="420" stylesheets="@Splash.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.kiluet.jguitar.desktop.SplashController">
  <VBox fx:id="splashBox" styleClass="splashBox">
    <children>
      <ImageView fx:id="imageView">
        <image>
          <Image url="com/kiluet/jguitar/desktop/jguitar2.png" backgroundLoading="true" />
        </image>
      </ImageView>
      <ProgressBar fx:id="progressBar" prefWidth="400" />
    </children>
  </VBox>
</Pane>




