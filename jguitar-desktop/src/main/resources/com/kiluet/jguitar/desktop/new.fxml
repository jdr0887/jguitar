<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="140" prefWidth="240" stylesheets="@new.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.kiluet.jguitar.desktop.NewController">
  <children>
    <GridPane alignment="CENTER" hgap="10" vgap="10">
      <padding>
        <Insets top="25" right="25" bottom="10" left="25" />
      </padding>
      <Label text="Name:" GridPane.rowIndex="0" GridPane.columnIndex="0" />
      <TextField fx:id="name" GridPane.rowIndex="0" GridPane.columnIndex="1" />
      <HBox spacing="50" alignment="CENTER" GridPane.columnSpan="2" GridPane.columnIndex="0" GridPane.rowIndex="3">
        <Button text="OK" onAction="#doOK" />
        <Button text="Cancel" onAction="#doCancel" />
      </HBox>
    </GridPane>
  </children>
</AnchorPane>